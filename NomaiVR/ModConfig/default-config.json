{
  "requireVR": true,
  "enabled": true,
  "settings": {
    "refreshRateOverride": {
      "type": "selector",
      "value": 0,
      "options": [0, 30, 60, 70, 72, 80, 90, 120, 144],
      "title": "Phyics refresh rate (zero = sync with HMD)"
	},
    "vibrationIntensity": {
      "type": "slider",
      "value": 1,
      "min": 0,
      "max": 3,
      "title": "Vibration Intensity"
	},
    "helmetVisibility": {
      "type": "toggle",
      "value": false,
      "title": "Helmet around head",
      "yes": "Visible",
      "no": "Hidden"
    },
    "hideHudDuringDialogue": {
      "type": "toggle",
      "value": false,
      "title": "HUD during conversations",
      "yes": "Hidden",
      "no": "Visible"
    },
    "movementControllerOriented": {
      "type": "toggle",
      "value": false,
      "title": "Movement orientation",
      "yes": "Left controller",
      "no": "Head"
    },
    "showGesturePrompts": {
      "type": "toggle",
      "value": true,
      "title": "VR gesture tutorials",
      "yes": "Enabled",
      "no": "Disabled"
    },
    "disableCursorLock": {
      "type": "toggle",
      "value": true,
      "title": "Lock mouse cursor to window",
      "yes": "Unlocked",
      "no": "Locked"
    },
    "debug": {
      "type": "toggle",
      "value": true,
      "title": "Debug mode (more logs)",
      "yes": "Enabled",
      "no": "Disabled"
    }
  }
}
